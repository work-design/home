<%= form_with theme: 'weui', model: [:my, @escort] do |f| %>
  <%= render 'shared/error_messages', target: @escort %>
  <%= f.text_field :from_name %>
  <%= f.text_field :from_mobile %>
  <%= f.text_field :from_address %>
  <%= f.text_field :to_name %>
  <%= f.text_field :to_mobile %>
  <%= f.text_field :to_address %>
  <%= f.text_area :goods, class: 'weui-textarea' %>
  <%= f.text_field :car %>
  <%= f.text_area :note, class: 'weui-textarea' %>
  <%= f.text_field :state %>
  <%= f.submit %>
<% end %>
