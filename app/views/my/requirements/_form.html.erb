<%= form_with theme: 'weui', model: [:my, @requirement] do |f| %>
  <%= render 'shared/error_messages', target: @requirement %>
  <%= f.text_field :name %>
  <%= f.text_field :from %>
  <%= f.text_field :to %>
  <%= f.date_select :pick_on %>
  <%= f.time_select :pick_at %>
  <%= f.text_area :note %>
  <%= f.file_field :credential %>
  <%= f.submit %>
<% end %>
