<%= form_with model: [:admin, @requirement] do |f| %>
  <%= render 'shared/error_messages', target: @requirement %>
  <%= f.text_field :name %>
  <%= f.text_field :mobile %>
  <%= f.text_field :from %>
  <%= f.text_field :to %>
  <%= f.text_field :pick_on %>
  <%= f.text_field :pick_at %>
  <%= f.text_field :note %>
  <%= f.submit %>
<% end %>
